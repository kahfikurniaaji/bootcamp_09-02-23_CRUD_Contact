<table border="1">
    <tr>
        <th>Nama</th>
        <th>No Telp</th>
    </tr>
    <% if (contacts.length) { %>
        <% contacts.forEach(contact=> { %>
            <tr>
                <td>
                    <%=contact.name %>
                </td>
                <td>
                    <%=contact.mobile %>
                </td>
            </tr>
            <% }); %>
                <% } else {%>
                    <tr>
                        <td colspan="2">Kontak kosong</td>
                    </tr>
                    <% } %>
</table>